<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="com.tle.core.usermanagement.leap" version="1">
	<requires>
		<import plugin-id="com.tle.core.guice" />
		<import plugin-id="com.tle.core.entity.services" />
		<import plugin-id="com.tle.core.settings" />
		<import plugin-id="com.tle.core.usermanagement" />
		<import plugin-id="com.tle.common.usermanagement.leap" />
	</requires>

	<runtime>
		<library type="code" path="classes/" id="core" />
	</runtime>

	<extension plugin-id="com.tle.core.guice" point-id="module" id="guiceModules" />

	<extension plugin-id="com.tle.core.usermanagement" point-id="userManager" id="leap">
		<parameter id="class" value="bean:com.tle.core.usermanagement.leap.wrapper.LeapUserWrapper" />
		<parameter id="settingsClass" value="com.tle.beans.usermanagement.leap.wrapper.LeapUserWrapperSettings" />
		<parameter id="order" value="1050" />
	</extension>
</plugin>
