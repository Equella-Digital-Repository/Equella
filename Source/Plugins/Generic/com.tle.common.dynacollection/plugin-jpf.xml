<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="com.tle.common.dynacollection" version="1">
	<requires>
		<import plugin-id="com.tle.common.search" />
		<import plugin-id="com.tle.common.schema" />
		<import plugin-id="com.tle.common.entity" />
		<import plugin-id="com.tle.common.collection" />
	</requires>

	<runtime>
		<library type="code" path="classes/" id="core">
			<export prefix="*" />
		</library>
	</runtime>

	<extension-point id="usages">
		<parameter-def id="nameKey" />
	</extension-point>
</plugin>
