{
  "private": true,
  "config": {
    "buildcss": "target/resources/web/css",
    "devcss": "../target/scala-2.12/classes/web/css",
    "buildlang": "target/resources/lang",
    "devlang": "../target/scala-2.12/classes/lang"
  },
  "scripts": {
    "install": "cross-env-shell \"bower --allow-root install && mkdirp ${npm_package_config_devlang} ${npm_package_config_buildlang} ${npm_package_config_devcss} ${npm_package_config_buildcss}\"",
    "clean": "rm -rf target/ output/ node_modules/ bower_components/ && yarn install",
    "build": "yarn install && tsc && yarn run build:pages && yarn run build:scss && yarn run build:langbundle",
    "build:pages": "yarn run build:index && yarn run build:uploadlist",
    "build:index": "node build-bundle.js IndexPage index.js",
    "dev:index": "node build-bundle.js --dev IndexPage index.js",
    "build:uploadlist": "node build-bundle.js --lib Uploads.UploadList uploadlist.js",
    "dev:uploadlist": "node build-bundle.js --dev --lib Uploads.UploadList uploadlist.js",
    "build:scss": "cross-env-shell \"node-sass sass/styles.scss ${npm_package_config_buildcss}/styles-new.css\"",
    "dev:scss": "cross-env-shell \"node-sass --watch sass/styles.scss ${npm_package_config_devcss}/styles-new.css\"",
    "build:langbundle": "cross-env-shell \"pulp build -O --to output/genlang.js -m Tools.GenLangStrings && node output/genlang.js > ${npm_package_config_buildlang}/jsbundle.json\"",
    "dev:langbundle": "cross-env-shell \"pulp build -O --to output/genlang.js -m Tools.GenLangStrings && node output/genlang.js > ${npm_package_config_devlang}/jsbundle.json\""
  },
  "dependencies": {
    "@types/react-redux": "^5.0.7",
    "@types/react-router": "^4.0.22",
    "@types/react-router-dom": "^4.2.4",
    "@types/react-router-redux": "^5.0.12",
    "@types/redux-logger": "^3.0.5",
    "axios": "^0.18.0",
    "create-react-class": "^15.6.2",
    "date-fns": "^2.0.0-alpha.7",
    "history": "^4.7.2",
    "material-ui": "1.0.0-beta.38",
    "material-ui-pickers": "^1.0.0-rc.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts-ts": "^2.13.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "timeago-react": "^2.0.0"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^22.1.3",
    "@types/node": "^9.4.6",
    "@types/react": "^16.0.40",
    "@types/react-dom": "^16.0.4",
    "argparse": "^1.0.10",
    "bower": "^1.8.2",
    "browserify": "^16.1.0",
    "combined-stream2": "^1.1.2",
    "cross-env": "^5.1.3",
    "cross-spawn": "^6.0.5",
    "envify": "^4.1.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.7.2",
    "pulp": "^12.0.1",
    "purescript": "^0.11.7",
    "typescript": "^2.7.2",
    "uglify-es": "^3.3.9",
    "uglifyify": "^4.0.5"
  }
}
