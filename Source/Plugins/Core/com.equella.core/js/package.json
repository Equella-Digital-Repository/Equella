{
  "config": {
    "buildcss": "target/web/css",
    "devcss": "../target/scala-2.12/classes/web/css",
    "buildlang": "target/lang",
    "devlang": "../target/scala-2.12/classes/lang"
  },
  "scripts": {
    "install": "cross-env-shell \"bower --allow-root install && mkdirp ${npm_package_config_devlang} ${npm_package_config_buildlang} ${npm_package_config_devcss} ${npm_package_config_buildcss}\"",
    "clean": "rm -rf target/ output/ node_modules/ bower_components/ && yarn install",
    "build": "yarn install && yarn run build:pages && yarn run build:scss && yarn run build:langbundle",
    "build:pages": "yarn run build:legacy && yarn run build:search && yarn run build:settings && yarn run build:uploadlist",
    "build:legacy": "node build-bundle.js LegacyPage index.js",
    "dev:legacy": "node build-bundle.js --dev LegacyPage index.js",
    "build:search": "node build-bundle.js SearchPage searching.js",
    "dev:search": "node build-bundle.js --dev SearchPage searching.js",
    "build:settings": "node build-bundle.js SettingsPage settings.js",
    "dev:settings": "node build-bundle.js --dev SettingsPage settings.js",
    "build:uploadlist": "node build-bundle.js --lib Uploads.UploadList uploadlist.js",
    "dev:uploadlist": "node build-bundle.js --dev --lib Uploads.UploadList uploadlist.js",
    "build:scss": "cross-env-shell \"node-sass sass/styles.scss ${npm_package_config_buildcss}/styles-new.css\"",
    "dev:scss": "cross-env-shell \"node-sass sass/styles.scss ${npm_package_config_devcss}/styles-new.css\"",
    "build:langbundle": "cross-env-shell \"pulp build -O --to output/genlang.js -m Tools.GenLangStrings && node output/genlang.js > ${npm_package_config_buildlang}/jsbundle.json\"",
    "dev:langbundle": "cross-env-shell \"pulp build -O --to output/genlang.js -m Tools.GenLangStrings && node output/genlang.js > ${npm_package_config_devlang}/jsbundle.json\""
  },
  "dependencies": {
    "create-react-class": "^15.6.2",
    "material-ui": "1.0.0-beta.35",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "timeago-react": "^2.0.0"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "argparse": "^1.0.10",
    "bower": "^1.8.2",
    "browserify": "^16.1.0",
    "combined-stream2": "^1.1.2",
    "cross-env": "^5.1.3",
    "cross-spawn": "^6.0.5",
    "envify": "^4.1.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.7.2",
    "pulp": "^12.0.1",
    "purescript": "^0.11.7",
    "uglify-es": "^3.3.9",
    "uglifyify": "^4.0.5"
  }
}
