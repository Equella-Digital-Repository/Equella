{
  "config": {
    "buildjs": "target/web/reactjs",
    "devjs": "../target/scala-2.11/classes/web/reactjs"
  },
  "scripts": {
    "install": "cross-env-shell \"bower --allow-root install && mkdirp ${npm_package_config_devjs} ${npm_package_config_buildjs}\"",
    "clean": "rm -rf target/ output/ node_modules/ bower_components/ && yarn install",
    "build": "yarn install && yarn run build:uploadlist",
    "build:uploadlist": "cross-env-shell \"pulp browserify --optimise -m Uploads.UploadList --standalone PS | browserify -g [ envify --NODE_ENV production ] -g uglifyify - | uglifyjs --compress --mangle > ${npm_package_config_buildjs}/uploadlist.js\"",
    "dev:uploadlist": "cross-env-shell \"pulp -w browserify --optimise -m Uploads.UploadList --standalone PS --to ${npm_package_config_devjs}/uploadlist.js\""
  },
  "dependencies": {
    "create-react-class": "^15.6.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "bower": "^1.8.2",
    "browserify": "^16.1.0",
    "cross-env": "^5.1.3",
    "envify": "^4.1.0",
    "mkdirp": "^0.5.1",
    "pulp": "^12.0.1",
    "purescript": "^0.11.7",
    "uglify-es": "^3.3.9",
    "uglifyify": "^4.0.5"
  }
}
