<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="com.sun.syndication" version="1">
	<requires>
		<import plugin-id="jdom:jdom" />
		<import plugin-id="com.tle.common.licences" />
		<import plugin-id="com.sun.syndication:rome" exported="true" />
	</requires>

	<extension plugin-id="com.tle.common.licences" point-id="licenceExtension" id="romeLicenceName">
		<parameter id="thirdPartyName" value="Rome" />
		<parameter id="licenceName" value="Apache 2.0" />
		<parameter id="licenceURLInternal" value="apache-2.0.txt" /><!-- relative to com.tle.web.licence/resources/web/documentation -->
		<parameter id="licenceURLExternal" value="http://www.apache.org/licenses/LICENSE-2.0.html" />
		<parameter id="thirdPartyHome" value="https://rome.dev.java.net/ " />
	</extension>
</plugin>
