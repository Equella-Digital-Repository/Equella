<?xml version="1.0" encoding="UTF-8"?>
<project name="common">
	<dirname property="this.dir" file="${ant.file.common}"/>
	<import file="${this.dir}/common-ant.xml" />

	<path id="extra.classpath" />

	<target name="define-build.classpath" depends="define-eclipseClasspath">
		<path id="build.classpath">
			<path refid="eclipse.classpath" />
			<path refid="extra.classpath" />
		</path>
	</target>

	<target name="clean" depends="common-ant.clean" />

	<target name="init" depends="common-ant.init" />
	
	<target name="compile" depends="common-ant.compile" if="src.enabled"/>

	<target name="make" depends="common-ant.make" />

	<target name="product" depends="test" />
</project>
