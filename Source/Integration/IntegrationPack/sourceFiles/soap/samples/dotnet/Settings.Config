<?xml version="1.0"?>
<appSettings>
  <add key="username" value="someusername" />
  <add key="password" value="somepassword" />

  <!-- Proxies.  Uncomment to use -->
  <!--
  <add key="proxyUrl" value="http://localhost:8888"/>
  <add key="proxyUsername" value="proxyUser"/>
  <add key="proxyPassword" value="password"/>
  -->

  <!-- You can specify useTokens = true to append a single-sign-on token to the results of the search result URLs.  
  Note that to use this functionality, the Shared Secrets user management plugin must be enabled 
  (see User Management in the EQUELLA Administration Console) and a shared secret must be configured. -->
  <add key="useTokens" value="false"/>
  <add key="tokenUser" value="someUsername"/>
  <add key="sharedSecretId" value="someSecretId"/>
  <add key="sharedSecretValue" value="someSecretValue"/>
</appSettings>