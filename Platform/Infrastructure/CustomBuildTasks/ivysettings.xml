<?xml version="1.0" encoding="UTF-8"?>
<ivy-settings>
  <property name="local-maven2-dir" value="${user.home}/.local-repo/" />
  <property name="equelladeps-dir" value="${user.home}/equella-deps/" />
	
  <settings defaultResolver="main" />
  <resolvers>
    <chain name="main">
      <ibiblio name="central" m2compatible="true"/>
      <ibiblio name="internaldeps" m2compatible="true" root="file://${equelladeps-dir}" />
    </chain>
  </resolvers>
</ivy-settings>
