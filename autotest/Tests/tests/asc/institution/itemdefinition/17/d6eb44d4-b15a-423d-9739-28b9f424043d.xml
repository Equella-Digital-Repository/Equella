                  <string>&lt;script&gt;
function doSearch()
{
	var q = $(&apos;#query&apos;).val();
	
	$.post(&apos;p/geturl?url=&apos; + encodeURIComponent($(&apos;#submitto&apos;).val()), 
		{echo: q},
		function(data){
			var res = $(&apos;#results&apos;);
			res.empty();
			res.append($(data).find(&apos;#echoed&apos;).text());
		}, &apos;html&apos; 
		);
}
&lt;/script&gt;

&lt;div id=&quot;${prefix}&quot; class=&quot;ctrlbody&quot;&gt;
     
Echo Server Url:
&lt;input type=&quot;text&quot; id=&quot;submitto&quot; name=&quot;submitto&quot;&gt;
&lt;br&gt;
To Echo:
&lt;input type=&quot;text&quot; id=&quot;query&quot; name=&quot;query&quot;&gt;

&lt;br&gt;
&lt;input type=&quot;button&quot; id=&quot;search&quot; name=&quot;search&quot; value=&quot;Do it&quot; onclick=&quot;doSearch(); return false;&quot; class=&quot;button-big&quot;&gt;
&lt;/div&gt;

&lt;div id=&quot;results&quot;&gt;
&lt;/div&gt;</string>
