          </bundleTitle>
          <configuration>&lt;xml&gt;&lt;markup&gt;&amp;lt;div id=&amp;quot;related&amp;quot;&amp;gt;
&amp;lt;h3&amp;gt; Also by this contributor... &amp;lt;/h3&amp;gt;
&amp;lt;#if attributes.get(&amp;quot;more/hasresults&amp;quot;) == &apos;true&apos; &amp;gt;
	&amp;lt;span id=&amp;quot;results&amp;quot; style=&amp;quot;margin-top: 7px; display: block;&amp;quot; &amp;gt;	
		&amp;lt;ul style=&amp;quot;list-style-type: square&amp;quot;&amp;gt;
			&amp;lt;#list attributes.getAllSubtrees(&amp;quot;more/results/result&amp;quot;) as result &amp;gt;
				&amp;lt;li id=&amp;quot;r${result_index}&amp;quot; style=&amp;quot;padding: 5px 5px 5px 0; margin-left:15px;&amp;quot;&amp;gt;&amp;lt;a href=&amp;quot;items/${result.get(&apos;uuid&apos;)}/${result.get(&apos;version&apos;)}&amp;quot;&amp;gt;${result.get(&apos;name&apos;)}&amp;lt;/a&amp;gt;&amp;lt;/li&amp;gt;
			&amp;lt;/#list&amp;gt;
		&amp;lt;/ul&amp;gt;
	&amp;lt;/span&amp;gt;
&amp;lt;#else&amp;gt;
	&amp;lt;span id=&amp;quot;no-results&amp;quot; style=&amp;quot;margin-top: 7px; display: block;&amp;quot;&amp;gt;There are no more items by this contributor&amp;lt;/span&amp;gt;
&amp;lt;/#if&amp;gt;
&amp;lt;/span&amp;gt;
&amp;lt;/div&amp;gt;
&lt;/markup&gt;&lt;script&gt;search = utils.searchAdvanced(&amp;quot;&amp;quot;,&amp;quot;/xml/item/owner IS &apos;&amp;quot; + item.getOwner() + &amp;quot;&apos; AND /xml/item/@id IS NOT &apos;&amp;quot; + item.getUuid() + &amp;quot;&apos;&amp;quot; , true, 0, false, 0, 5);
if(search.available() &amp;gt; 0 )
{
	attributes.set(&amp;quot;more/hasresults&amp;quot;, &apos;true&apos;);
	xmlResults = attributes.createSubtree(&amp;quot;more/results&amp;quot;);
	results = search.getResults();
	for(i=0;i&amp;lt;results.size();i++)
	{
		searchresult = results.get(i);
		itemInfo = xmlResults.createSubtree(&amp;quot;result&amp;quot;);
		itemInfo.set(&amp;quot;name&amp;quot;, searchresult.getName());
		itemInfo.set(&amp;quot;uuid&amp;quot;, searchresult.getUuid());
		itemInfo.set(&amp;quot;version&amp;quot;, searchresult.getVersion());
	}
}
else
{
	attributes.set(&amp;quot;more/hasresults&amp;quot;, &apos;false&apos;);
}
&lt;/script&gt;&lt;/xml&gt;</configuration>
